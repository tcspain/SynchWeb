<extendedCollectRequest>
    <collect_request>
        <fileinfo>
        <directory><%-DIR%></directory>
        <prefix><%-IMP%></prefix>
        </fileinfo>
        <oscillation_sequence>
            <start><%-AXISSTART%></start>
            <range><%-AXISRANGE%></range>
            <number_of_images><%-NUMIMG%></number_of_images>
            <overlap><%-OVERLAP%></overlap>
            <exposure_time><%-EXPOSURETIME%></exposure_time>
            <start_image_number>1</start_image_number>
            <number_of_passes>1</number_of_passes>
        </oscillation_sequence>
        <wavelength><%-WAVELENGTH%></wavelength>
        <resolution>
            <upper><%-RESOLUTION%></upper>
        </resolution>
        <sample_reference>
            <container_reference><%-CPOS%></container_reference>
            <sample_location><%-SPOS%></sample_location>
            <blSampleId><%-BLSAMPLEID%></blSampleId>
        </sample_reference>
    </collect_request>
    <runNumber>0</runNumber>
    <sampleDetectorDistanceInMM><%-DET%></sampleDetectorDistanceInMM>
    <transmissionInPerCent><%-TRANSMISSION%></transmissionInPerCent>
    <sampleName><%-SAMPLE%></sampleName>
    <visitPath><%-VISITPATH%></visitPath>
    <comment></comment>
    <suffix>cbf</suffix>
    <totalNumberOfImages><%-NUMIMG%></totalNumberOfImages>
    <fileNameTemplate><%-DIR+IMP%>_0_%04cbf</fileNameTemplate>
    <hasBeamSize>false</hasBeamSize>
    <hasTransmission>true</hasTransmission>
    <!--<aperturePosition><%-AP%></aperturePosition>-->
</extendedCollectRequest>
